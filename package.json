{
  "name": "fluentui-icons-mcp-app",
  "version": "1.0.1",
  "type": "module",
  "description": "MCP App for Fluent UI React V9 - Icon Explorer",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc --noEmit && cross-env INPUT=mcp-app.html vite build && tsc -p tsconfig.server.json",
    "watch": "cross-env INPUT=mcp-app.html vite build --watch",
    "serve": "tsx main.ts",
    "start": "node dist/main.js",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run watch\" \"npm run serve\"",
    "generate:icons": "tsx scripts/generate-icon-list.ts",
    "generate:sizes": "tsx scripts/generate-icon-sizes.ts",
    "generate:registry": "tsx scripts/generate-icon-registry.ts",
    "generate:tags": "tsx scripts/generate-icon-visual-tags.ts",
    "generate:all": "npm run generate:icons && npm run generate:sizes && npm run generate:registry && npm run generate:tags"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.72.11",
    "@fluentui/react-icons": "^2.0.317",
    "@modelcontextprotocol/ext-apps": "^1.0.1",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "fuse.js": "^7.1.0",
    "natural": "^8.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "synonyms": "^1.0.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-singlefile": "^2.3.0"
  }
}
