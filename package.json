{
  "name": "fluentui-icons-mcp-app",
  "version": "1.0.0",
  "type": "module",
  "description": "MCP App for Fluent UI React V9 - Icon Explorer",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc --noEmit && cross-env INPUT=mcp-app.html vite build && tsc -p tsconfig.server.json",
    "watch": "cross-env INPUT=mcp-app.html vite build --watch",
    "serve": "tsx main.ts",
    "start": "cross-env NODE_ENV=development npm run build && npm run serve",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run watch\" \"npm run serve\"",
    "generate:icons": "tsx scripts/generate-icon-list.ts",
    "generate:sizes": "tsx scripts/generate-icon-sizes.ts",
    "generate:registry": "tsx scripts/generate-icon-registry.ts",
    "generate:tags": "tsx scripts/generate-icon-visual-tags.ts",
    "generate:all": "npm run generate:icons && npm run generate:sizes && npm run generate:registry && npm run generate:tags"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.56.0",
    "@fluentui/react-icons": "^2.0.268",
    "@modelcontextprotocol/ext-apps": "^1.0.0",
    "@modelcontextprotocol/sdk": "^1.24.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "fuse.js": "^7.1.0",
    "natural": "^8.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "synonyms": "^1.0.1",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.0",
    "@types/express-rate-limit": "^5.1.3",
    "@types/natural": "^5.1.5",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vite-plugin-singlefile": "^2.0.0"
  }
}
